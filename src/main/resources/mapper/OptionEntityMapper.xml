<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.startpractice.dao.OptionEntityMapper">
    <resultMap id="BaseResultMap" type="com.example.startpractice.dao.entity.OptionEntity">
        <result column="option_content" jdbcType="VARCHAR" property="optionContent"/>
        <result column="checked" jdbcType="VARCHAR" property="checked"/>
        <result column="question_id" jdbcType="VARCHAR" property="questionId"/>
    </resultMap>
    <sql id="Base_Column_List">
        option_content, checked, question_id
    </sql>

    <insert id="insert" parameterType="com.example.startpractice.dao.entity.OptionEntity">

        insert into option_info (option_content, checked, question_id)
        values (#{optionContent,jdbcType=VARCHAR}, #{checked,jdbcType=VARCHAR},
                  #{questionId,jdbcType=VARCHAR})
    </insert>

</mapper>